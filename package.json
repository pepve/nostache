{
  "name": "nostache",
  "version": "0.0.3",
  "description": "Mustache clone for Node.js",
  "main": "lib/runtime",
  "scripts": {
    "pretest": "jshint lib",
    "test": "nodeunit test",
    "coveralls": "jscoverage lib && NOSTACHE_COVERAGE=1 nodeunit --reporter=lcov test | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pepve/nostache.git"
  },
  "author": "Pepijn Verlaan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pepve/nostache/issues"
  },
  "devDependencies": {
    "nodeunit": "^0.9.0",
    "jshint": "^2.5.5",
    "jscoverage": "^0.5.6",
    "coveralls": "^2.11.2"
  },
  "jshintConfig": {
    "node": true,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": "nofunc",
    "newcap": true,
    "noempty": true,
    "quotmark": "single",
    "undef": true,
    "unused": true
  }
}
